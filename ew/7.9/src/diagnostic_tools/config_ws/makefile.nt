#                    Nmake File For config_ws - Windows NT version
#
NODEBUG=1

!include <ntwin32.mak>

APP = config_ws
O = $(APP).obj

B = $(EW_HOME)\$(EW_VERSION)\bin
L = $(EW_HOME)\$(EW_VERSION)\lib


$B\$(APP).exe: $O
   $(link) $(conlflags) $(ldebug) $(conlibsmt) $O \
    $L\trheadconv.obj \
    $L\libew_mt.lib $L\mem_circ_queue.obj $L\swap.obj \
    -out:$B\$(APP).exe

.c.obj:
   $(CC) $(GLOBALFLAGS)  /nologo $(cflags) $(cdebug) $(cvarsmt) $(tflags) $<

sniffring: sniffring.obj
        ilink $(LFLAGS) sniffring.obj getutil.obj kom.obj transport.obj sleep_ew.obj

# Clean-up directives
clean:
	-del a.out core *.o *.obj *% *~

clean_bin:
	-del $B\config_ws*
