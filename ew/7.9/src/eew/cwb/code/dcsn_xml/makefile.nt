#
#                    Nmake File For dcsn_xml  - Windows NT version
#
NODEBUG=1

!include <ntwin32.mak>

APP = dcsn_xml
O = $(APP).obj 

B = $(EW_HOME)\$(EW_VERSION)\bin
L = $(EW_HOME)\$(EW_VERSION)\lib



$B\$(APP).exe: $O
    $(link) $(conlflags) $(ldebug) $(conlibsmt) $O $L\kom.obj $L\getutil.obj \
    $L\time_ew.obj $L\chron3.obj $L\logit.obj $L\transport.obj $L\sleep_ew.obj \
    $L\swap.obj $L\lockfile.obj $L\lockfile_ew.obj \
	-out:dcsn_xml_v1.exe

.c.obj:
   $(cc) $(GLOBALFLAGS) /D_CRT_SECURE_NO_DEPRECATE  /nologo $(cflags) $(cdebug) $(cvarsmt) $(tflags) $<


# Clean-up directives
clean:
	-del a.out core *.o *.obj *% *~

clean_bin:
	-del $B\dcsn_xml*
